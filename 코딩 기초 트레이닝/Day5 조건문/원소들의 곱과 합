def solution(num_list):
    sum=0
    mul=1
    for num in num_list :
        sum+=num
        mul*=num
    
    if mul < sum*sum :
        return 1
    else :
        return 0
    
    # return int(mul < sum(num_list) ** 2)
    # s=sum(num_list)**2
    # m=eval('*'.join([str(n) for n in num_list]))
    # return 1 if s>m else 0