def solution(arr, intervals):
    # #원소 접근을 어떻게..
    # for a,b in enumerate(intervals):
    #     answer.append(arr)
    
    a=arr[intervals[0][0]:intervals[0][1]+1]
    b=arr[intervals[1][0]:intervals[1][1]+1]

    return a+b
